{
  "version": 3,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    },
    {
      "src": "dist/client/**/*",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/server/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
